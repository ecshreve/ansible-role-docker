{
	"name": "box",
	"build": {
    "dockerfile": "Dockerfile"
  },
	"features": {
		"ghcr.io/devcontainers/features/docker-outside-of-docker:1": {}
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"redhat.ansible",
				"GitHub.copilot",
				"mutantdino.resourcemonitor",
				"Gruntfuggly.todo-tree",
				"esbenp.prettier-vscode"
			]
		}
	},
	"runArgs": ["--hostname", "box"]
}